{
	"name": "eslint-plugin-best-practice",
	"version": "0.0.0",
	"description": "an eslint plugin to enforce some best practices",
	"license": "MIT",
	"repository": "smeijer/eslint-plugin-best-practice",
	"funding": "https://github.com/smeijer/eslint-plugin-best-practice?sponsor=1",
	"author": "Stephan Meijer <stephan.meijer@gmail.com> (meijer.ws)",
	"bugs": "https://github.com/smeijer/eslint-plugin-best-practice/issues",
	"homepage": "https://github.com/smeijer/eslint-plugin-best-practice#readme",
	"type": "module",
	"exports": {
