	"devDependencies": {
		"@smeijer/eslint-config": "^1.0.2",
		"@smeijer/tsconfig": "^1.0.0",
		"@types/jest": "^29.5.2",
		"jest": "^29.5.0",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
